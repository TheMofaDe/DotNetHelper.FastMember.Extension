{
  "metadata": [
    {
      "src": [
        {
          "files": [ "**.csproj" ],
          "exclude": [ "**/bin/**", "**/obj/**" ],
          "src": "../src"
        }
      ],
      "dest": "api",
      "properties": {
        "TargetFramework": "net452"
      }
    }
  ],
  "build": {

    "content": [
      {
        "files": [ "*.yml", "*.md" ],
        "src": "api",
        "dest": "api"
      },
      {
        "files": [ "*.md", "toc.yml" ],
        "src": ".",
        "dest": "."
      },

      {
        "files": [ "*.md", "*.yml", "**/*.md", "**/*.yml" ],
        "src": "./tutorials",
        "dest": "./tutorials"
      }
    ],
    "resource": [
      {
        "files": [ "images/**", "langwordmapping/**" ],
        "exclude": [ "**.meta" ]
      }
    ],
    "globalMetadata": {
      "_appTitle": "DotNetHelper.FastMember.Extension",
      //"_gitContribute": {
      //  "repo": "https://github.com/TheMofaDe/DotNetHelper.FastMember.Extension",
      //  "branch": "master",
      //  "apiSpecFolder": "docs-ref-overwrite"
      //},
      "_disableContribution": false,
      "_disableBreadcrumb": false,
      "_appFaviconPath": "logo/favicon.ico",
      "_disableNavbar": false,
      "_enableSearch": true,
      "_navRel": "toc.html",
      //    "_appLogoPath": "logo/lucene-net-color.png",
      "_appFooter": "Copyright Â© 2020  <strong>Joseph McNeal Jr</strong>"
    },
    "dest": "./",
    "globalMetadataFiles": [],

    //"template": [ "default", "statictoc", "templates/material" ],
    "template": [ "default", "statictoc", "templates/darkfx" ],
    //"template": [ "default", "statictoc", "templates/darkfx" ],
    "postProcessors": [],
    "markdownEngineName": "markdig",
    "noLangKeyword": false,
    "xrefService": [ "https://xref.docs.microsoft.com/query?uid={uid}" ],
    "keepFileLink": true,
    "cleanupCacheHistory": true,
    "disableGitFeatures": false
  }
}
